	@InProceedings{Feldman2008,
		author = {{Feldman}, H.~A. and {Hudson}, M.~J. and {Watkins}, R.},
		title = "{Cosmic Flows on 100 Mpc/h scales}",
	    booktitle  = {Conference proceedings for the 43rd Rencontres de Moriond},
	    year       = {2008},
	    publisher  = {Th� Gioi Publishers},
	    address    = {La Thuile, Val d'Aosta, Italy},
	    Aannote	   = "The findings in this proceeding is to introduce an updated method of velocity weighting called the minimum variance weights and a redesigned window functions in order to more accurately measure the bulk flow. This updated method is not only sensitive to large-scale motion on the order of 100 $h^{-1} Mpc^{-1}$ but also reduces aliasing due to the shape of the window function. They found that this new method works best in large surveys with small errors, but found the results to be the same when the surveys were noisy or sparse."
	}

	@article{Watkins2009:LCDM+Challenge,
		author = {Watkins, Richard and Feldman, Hume A. and Hudson, Michael J.},
		title = "{Consistently large cosmic flows on scales of 100 $h^{-1}$ Mpc: a challenge for the standard {$\Lambda$CDM} cosmology}",
		volume = {392},
		number = {2},
		pages = {743-756},
		year = {2009},
		doi = {10.1111/j.1365-2966.2008.14089.x},
		URL = {http://mnras.oxfordjournals.org/content/392/2/743.abstract},
		eprint = {http://mnras.oxfordjournals.org/content/392/2/743.full.pdf+html},
		journal = {Monthly Notices of the Royal Astronomical Society},
		Aannote = "This article builds upon the {``Cosmic Flows on 100 Mpc/h scales"} paper published in the Conference proceedings for the 43rd Rencontres de Moriond. Using the minimum variance weight described in the proceedings, they apply this new method to all major peculiar velocity surveys and find that the majority of them are consistent with each other. However, the challenge that is presented is an excess in power in the bulk flow at around $600$ $h^{-1}$ $Mpc$ which is significantly larger than that predicted by $\Lambda$CDM model (otherwise known as the Standard Model of Cosmology) parameters given by the WMAP-5 Observations. No direct reason for this conflict is found, but its important to note that the majority of surveys are in agreement and no possible changes in the parameterization of the $\Lambda$CDM model allows it to agree with the results found in this paper."
		}

	@article{Macaulay21092012,
		author = {Macaulay, E. and Feldman, H. A. and Ferreira, P. G. and Jaffe, A. H. and Agarwal, S. and Hudson, M. J. and Watkins, R.},
		title = {Power Spectrum Estimation from Peculiar Velocity Catalogues},
		volume = {425},
		number = {3},
		pages = {1709-1717},
		year = {2012},
		doi = {10.1111/j.1365-2966.2012.21629.x},
		URL = {http://mnras.oxfordjournals.org/content/425/3/1709.abstract},
		eprint = {http://mnras.oxfordjournals.org/content/425/3/1709.full.pdf+html},
		journal = {Monthly Notices of the Royal Astronomical Society},
		Aannote = "Previously, large-scale cosmic flows were found to be in conflict with the predictions of the $\Lambda$CDM model (see reference [10]). In this paper, they compare the results of various peculiar velocity surveys using both the full catalogue directly and the minimum variance weights method. They find that minimum variance weights method achieves its desired result too well in that it preferentially weights large-scale motion by filtering at a significant amount of small-scale information. While at small-scales the catalogue method is in strong agreement with the predictions of the $\Lambda$CDM model, the minimum variance moments underestimates the power at this scale. What this underestimate means for the large-scale motion is that the small-scale motion that is filtered out by the moments method actually provides the necessary constraints that allow the bulk flow to agree with the $\Lambda$CDM model. Thus, they find that ultimately including more data from the velocity fields (i.e. the small-scale effects) improves the agreement with the $\Lambda$CDM model."
	}

