	@article{Watkins2007,
		author = {Watkins, R. and Feldman, H.~A.},
		title = {Power Spectrum Shape from Peculiar Velocity Data},
		volume = {379},
		number = {1},
		pages = {343-348},
		year = {2007},
		doi = {10.1111/j.1365-2966.2007.11970.x},
		URL = {\url{http://mnras.oxfordjournals.org/content/379/1/343.abstract}},
		eprint = {http://mnras.oxfordjournals.org/content/379/1/343.full.pdf+html},
		journal = {Monthly Notices of the Royal Astronomical Society},
		Aannote = "Peculiar velocity surveys often include non-linear effects which are present in small-scale flows. In order to use a linear theory of bulk flow, these small-scales must be filtered out. The method used in this paper is to form velocity moments by giving certain weight to the individual peculiar velocities that are reflective of the large scale motion. This is followed by a likelihood analysis where the maximum likelihood values for the moments and the small-scale noise are assessed. This allowed them to set cosmological constraints on the velocity power spectrum that reduces the influence of small-scale flows. This method showed that the difference velocity surveys were consistent in showing the same large-scale motion."
	}

